                                            <% for( let i = 0; i < posts.length; i++ ) { %>
                                                <% if (i % 3 == 0) { %>
                                                    <div class="image-row">
                                                <% } %>
                                                <div class="flex-1 has-background-image" data-background="data:image/<%=posts[i].img.contentType%>;base64,<%=posts[i].img.data.toString('base64')%>" >
                                                    <div class="overlay"></div>
                                                    <div class="image-owner">
                                                        <img class="avatar" src="/static/uploads/<%= posts[i].user.profilImage %>" alt="">
                                                        <div class="name"><% posts[i].user.username %></div>
                                                    </div>
                                                    <div class="photo-time"><% posts[i].date %></div>
                                                    <a class="photo-like">
                                                        <div class="inner">
                                                            <div class="like-overlay"></div>
                                                            <i data-feather="heart"></i>
                                                        </div>
                                                    </a>
                                                </div>
                                                <% if (i > 0 && (i + 1) % 3 == 0) { %>
                                                    </div>
                                                <% } %>
                                            <% } %>


