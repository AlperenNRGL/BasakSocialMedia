<!DOCTYPE html>
<html lang="tr">

<head>
    <!-- Required meta tags -->
    <%- include ("../partials/head") %> 
</head>

<body>
    <!-- Google Tag Manager (noscript) -->
    <!-- <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KQHJPZP" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript> -->
    <!-- End Google Tag Manager (noscript) -->

    <!-- Pageloader -->
    <div class="pageloader"></div>
    <div class="infraloader is-active"></div>
    <div class="app-overlay"></div>

    <%- include ("../partials/navbar")  %> 
    <%- include ("../partials/navbar2")  %> 
    
    <div class="view-wrapper">

        <!-- Container -->
        <div class="container is-custom">

            <!-- Profile page main wrapper -->
            <div id="profile-main" class="view-wrap is-headless">
                <%- include ("../partials/profile-top", {user : user}) %> 

                <div class="columns">
                    <div id="profile-timeline-widgets" class="column is-4">

                        <!-- Basic Infos widget -->
                        <!-- html/partials/pages/profile/timeline/widgets/basic-infos-widget.html -->


                        <div class="basic-infos-wrapper">
                            <div class="card is-profile-info">
                                
                                <%- include ("../partials/profile-friend-row") %> 


                                <div class="info-row">
                                    <div>
                                        <span>Biyografi</span>
                                        <a class="is-inverted"><%= user.biyografi %></a>
                                    </div>
                                    <img class="friend-icon"  src="/static/icons/biography.png" >
                                    <!-- <i class="mdi mdi-map-marker"></i> -->
                                </div>

                                <div class="info-row">
                                    <div>
                                        <span>Şehir</span>
                                        <a class="is-inverted"><%= user.city %></a>
                                    </div>
                                    <img class="friend-icon"  src="/static/icons/city.png" >
                                    <!-- <i class="mdi mdi-map-marker"></i> -->
                                </div>
                                <div class="info-row">
                                    <div>
                                        <span>Ülke</span>
                                        <a class="is-inverted"><%= user.country %></a>
                                    </div>
                                    <img class="friend-icon"  src="/static/icons/earth.png" >
                                </div>
                                <div class="info-row">
                                    <div>
                                        <span>Arkadaşlar</span>
                                        <a class="is-muted"><%= user.friends.length %>  arkadaş</a>
                                    </div>
                                    <img class="friend-icon"  src="/static/icons/chat.png" >
                                </div>
                            </div>
                        </div>
                        <!-- Photos widget -->
                        <!-- html/partials/pages/profile/timeline/widgets/photos-widget.html -->
                        <div class="box-heading">
                            <h4>Fotoğraflar</h4>
                            <div class="dropdown is-neutral is-spaced is-right dropdown-trigger">
                                <div>
                                    <div class="button">
                                        <i data-feather="more-vertical"></i>
                                    </div>
                                </div>
                                <div class="dropdown-menu" role="menu">
                                    <div class="dropdown-content">
                                        <a href="/user/photos/<%= user._id %>" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="image"></i>
                                                <div class="media-content">
                                                    <h3>Fotoğrafları Göster</h3>
                                                    <small>Tüm fotoğrafları Görmek İçin Tıkla</small>
                                                </div>
                                            </div>
                                        </a>
                                        <!-- <a href="#" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="tag"></i>
                                                <div class="media-content">
                                                    <h3>Tagged</h3>
                                                    <small>View photos you are tagged in.</small>
                                                </div>
                                            </div>
                                        </a>
                                        <a href="#" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="folder"></i>
                                                <div class="media-content">
                                                    <h3>Albums</h3>
                                                    <small>Open my albums.</small>
                                                </div>
                                            </div>
                                        </a> -->
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="is-photos-widget">
                            <% for( let i = 0; i < posts.length && i<12 ; i++ ) { %>
                                <% if (posts[i].img.data != "null") { %>
                                    <img src="data:image/<%=posts[i].img.contentType%>;base64,<%=posts[i].img.data.toString('base64')%>"  alt="">
                                <% } %>
                            <% } %>
                            <!-- <img src="https://via.placeholder.com/200x200" data-demo-src="assets/img/demo/widgets/photos/2.jpg" alt="">
                           
                            <img src="https://via.placeholder.com/200x200" data-demo-src="assets/img/demo/widgets/photos/12.jpg" alt=""> -->
                        </div>
                        <!-- Star friends widget -->
                        <!-- html/partials/pages/profile/timeline/widgets/star-friends-widget.html -->
                        <div class="box-heading">
                            <h4>Arkadaşlar</h4>
                            <div class="dropdown is-neutral is-spaced is-right dropdown-trigger">
                                <div>
                                    <div class="button">
                                        <i data-feather="more-vertical"></i>
                                    </div>
                                </div>
                                <div class="dropdown-menu" role="menu">
                                    <div class="dropdown-content">
                                        <a href="/user/friends/<%= user._id %>" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="users"></i>
                                                <div class="media-content">
                                                    <h3>Arkadaşları Göster</h3>
                                                    <small>Tüm Arkadaşları Görmek İçin Tıkla</small>
                                                </div>
                                            </div>
                                        </a>
                                        <!-- <a href="#" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="heart"></i>
                                                <div class="media-content">
                                                    <h3>Family</h3>
                                                    <small>View family members.</small>
                                                </div>
                                            </div>
                                        </a>
                                        <a href="#" class="dropdown-item">
                                            <div class="media">
                                                <i data-feather="briefcase"></i>
                                                <div class="media-content">
                                                    <h3>Work Relations</h3>
                                                    <small>View work related friends.</small>
                                                </div>
                                            </div>
                                        </a> -->
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="friend-cards-list">
                            <div class="card is-friend-card">

                                <% for( let i = 0; i < user.friends.length && i < 6; i++ ) { %>
                                    <div class="friend-item">
                                        <img src="/static/uploads/<%=user.friends[i].profilImage  %>" data-demo-src="assets/img/avatars/dan.jpg" alt="" data-user-popover="1">
                                        <div class="text-content">
                                            <a href="/user/<%= user.friends[i]._id %> "><%= user.friends[i].firstName %> <%= user.friends[i].lastName %> </a>
                                            <span><%= user.friends[i].username %></span>
                                        </div>
                                        <div class="star-friend">
                                            <i data-feather="star"></i>
                                        </div>
                                    </div>
                                <% } %>
                            </div>
                        </div>
                        <!-- Videos widget -->
                        <!-- html/partials/pages/profile/timeline/widgets/videos-widget.html -->
                    </div>

                    <div class="column is-8">
                        <div id="profile-timeline-posts" class="box-heading">
                            <h4>Paylaşımlar</h4>
                            <div class="button-wrap">
                                <!-- <button type="button" class="button is-active">Recent</button>
                                <button type="button" class="button">Popular</button> -->
                            </div>
                        </div>

                        <div class="profile-timeline">


                            <% posts.forEach(post => { %>
                                <div id="feed-post-1" class="card is-post">
                                    <!-- Main wrap -->
                                    <div class="content-wrap">
                                        <!-- Post header -->
                                        <div class="card-heading">
                                            <!-- User meta -->
                                            <div class="user-block">
                                                    <div class="image">
                                                        <img src="/static/uploads/<%= post.user.profilImage %>" data-demo-src="assets/img/avatars/dan.jpg"
                                                            data-user-popover="1" alt="">
                                                    </div>
                                                <div class="user-info">
                                                    <a href="/user/<%= post.user._id %>">
                                                        <%= post.user.username %>
                                                    </a>
                                                    <span class="time">
                                                        <%= post.getDate() %>
                                                    </span>
                                                </div>
                                            </div>
                                            <!-- Right side dropdown -->
                                            <!-- /partials/pages/feed/dropdowns/feed-post-dropdown.html -->
                                            <div class="dropdown is-spaced is-right is-neutral dropdown-trigger">
                                                <div>
                                                    <div class="button">
                                                        <i data-feather="more-vertical"></i>
                                                    </div>
                                                </div>
                                                <div class="dropdown-menu" role="menu">
                                                    <div class="dropdown-content">
                                                        <a href="/user/<%= post.user._id %>" class="dropdown-item">
                                                            <div class="media">
                                                                <i data-feather="bookmark"></i>
                                                                <div class="media-content">
                                                                    <h3>Profil</h3>
                                                                    <small>Profili Görüntelemek İçin Tıkla.</small>
                                                                </div>
                                                            </div>
                                                        </a>
                                                        <!-- <a class="dropdown-item">
                                                            <div class="media">
                                                                <i data-feather="bell"></i>
                                                                <div class="media-content">
                                                                    <h3>Notify me</h3>
                                                                    <small>Send me the updates.</small>
                                                                </div>
                                                            </div>
                                                        </a>
                                                        <hr class="dropdown-divider">
                                                        <a href="#" class="dropdown-item">
                                                            <div class="media">
                                                                <i data-feather="flag"></i>
                                                                <div class="media-content">
                                                                    <h3>Flag</h3>
                                                                    <small>In case of inappropriate content.</small>
                                                                </div>
                                                            </div>
                                                        </a> -->
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /Post header -->
                                
                                        <!-- Post body -->
                                        <div class="card-body">
                                            <!-- Post body text -->
                                            <div class="post-text">
                                                <p>
                                                    <%= post.text %>
                                                </p>
                                            </div>
                                            <!-- Featured image -->
                                            
                                            <div class="post-image">
                                                <% if (post.img.data != "null" ) { %>
                                                <!-- <a data-fancybox="post1" data-lightbox-type="comments" data-thumb="assets/img/demo/unsplash/1.jpg" href="https://via.placeholder.com/1600x900" data-demo-href="assets/img/demo/unsplash/1.jpg"> -->
                                                    <img src="data:image/<%=post.img.contentType%>;base64,<%=post.img.data.toString('base64')%>" alt="post_image">
                                                <!-- </a> -->
                                                <% } else { %>
                                                 <div style="height: 30px;"></div>
                                                <% } %>
                                
                                                <!-- Action buttons -->
                                                <!-- /partials/pages/feed/buttons/feed-post-actions.html -->
                                                <div class="like-wrapper">
                                                    <input type="hidden" value="<%= user._id %>">
                                                    <input type="hidden" value="<%= post._id %>">
                                                    <a onclick="like_btn(this)" class="like-button" >
                                                        <% if (post.like.find(l => l.user.username == myuser.username )) { %>
                                                            <img width="40px" height="40px" src="/static/uploads/icons8-like-64.png">
                                                        <% } else { %>
                                                            <img width="35px" height="35px" src="/static/uploads/icons8-like-64-2.png">
                                                        <% } %>
                                                        <span class="like-overlay"></span>
                                                    </a>
                                                </div>
                                                <script>
                                                    function like_btn(e){
                                                        const postid = e.previousElementSibling.value;
                                                        const userid = document.getElementById("userid").value;
                                                        const countlike = document.getElementById(postid);
                                                        if(e.children[0].attributes.src.nodeValue == "/static/uploads/icons8-like-64.png"){
                                                            e.children[0].remove();
                                                            e.insertAdjacentHTML("afterbegin",`<img width="40px" height="40px" src="/static/uploads/icons8-like-64-2.png">`)
                                                            fetch(`https://basaksocialmediaapi.herokuapp.com/remove-like/${postid}/${userid}`)
                                                            .then(data => {
                                                                countlike.innerHTML = (Number(countlike.innerHTML) - 1)
                                                            })
                                                            .catch(err => console.log(err))
                                                            return;
                                                            }
                                                        e.children[0].remove();
                                                        e.insertAdjacentHTML("afterbegin",`<img width="40px" height="40px" src="/static/uploads/icons8-like-64.png">`)
                                                        fetch(`https://basaksocialmediaapi.herokuapp.com/add-like/${postid}/${userid}`)
                                                        .then(data => {
                                                            add_nofication("like", userid, e.previousElementSibling.previousElementSibling.value )
                                                            countlike.innerHTML = (Number(countlike.innerHTML) + 1)
                                                        })
                                                        .catch(err => console.log(err))
                                                    }
                                                </script>
                                                <div class="fab-wrapper is-share">
                                                    <a href="javascript:void(0);" class="small-fab share-fab" data-modal="share-modal">
                                                        <i data-feather="link-2"></i>
                                                    </a>
                                                </div>
                                
                                                <div class="fab-wrapper is-comment">
                                                    <a href="javascript:void(0);" class="small-fab">
                                                        <i data-feather="message-circle"></i>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /Post body -->
                                
                                        <!-- Post footer -->
                                        <div class="card-footer">
                                            <!-- Followers avatars -->
                                            <div class="likers-group">
                                                <% for( let i = 0; i < post.like.length && i < 4; i++ ) { %> 
                                                    <img src="/static/uploads/<%= post.like[i].user.profilImage %>" data-demo-src="/static/dist/assets/img/avatars/dan.jpg"
                                                        data-user-popover="1" alt="">
                                                <% } %>
                                                <!-- <img src="https://via.placeholder.com/300x300" data-demo-src="/static/dist/assets/img/avatars/david.jpg"
                                                    data-user-popover="4" alt="">
                                                <img src="https://via.placeholder.com/300x300" data-demo-src="/static/dist/assets/img/avatars/edward.jpeg"
                                                    data-user-popover="5" alt="">
                                                <img src="https://via.placeholder.com/300x300" data-demo-src="/static/dist/assets/img/avatars/milly.jpg"
                                                    data-user-popover="7" alt=""> -->
                                            </div>
                                            <!-- Followers text -->
                                            <!-- <div class="likers-text">
                                                <p>
                                                    <a href="#">Milly</a>,
                                                    <a href="#">David</a>
                                                </p>
                                                <p>and 23 more liked this</p>
                                            </div> -->
                                            <!-- Post statistics -->
                                            <div class="social-count">
                                                <div class="likes-count">
                                                    <i data-feather="heart"></i>
                                
                                                    <span class="countlike" id="<%= post._id %>" ><%= post.like.length %></span>
                                                </div>
                                                <div class="shares-count">
                                                    <i data-feather="link-2"></i>
                                                    <span>not now</span>
                                                </div>
                                                <div class="comments-count">
                                                    <i data-feather="message-circle"></i>
                                                    <span id="commentlike"><%= post.comments.length %></span>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- /Post footer -->
                                    </div>
                                    <!-- /Main wrap -->
                                
                                    <!-- Post #1 Comments -->
                                    <div class="comments-wrap is-hidden">
                                        <!-- Header -->
                                        <div class="comments-heading">
                                            <h4>Yorumlar
                                                <small>(<%= post.comments.length %> )</small>
                                            </h4>
                                            <div class="close-comments">
                                                <i data-feather="x"></i>
                                            </div>
                                        </div>
                                        <!-- /Header -->
                                
                                        <!-- Comments body -->
                                        <div class="comments-body has-slimscroll" id="post-<%= post._id %>">
                                
                                            <!-- Comment -->
                                            <% post.comments.forEach(comment=> { %>
                                                <div class="media is-comment">
                                                    <!-- User image -->
                                                    <div class="media-left">
                                                        <div class="image">
                                                            <img src="/static/uploads/<%= comment.user.profilImage %> "
                                                                data-demo-src="assets/img/avatars/dan.jpg" data-user-popover="1" alt="">
                                                        </div>
                                                    </div>
                                                    <!-- Content -->
                                                    <div class="media-content">
                                                        <a href="#">
                                                            <%= comment.user.username %>
                                                        </a>
                                                        <span class="time">
                                                            <%= comment.getDate() %>
                                                        </span>
                                                        <p>
                                                            <%= comment.text %>
                                                        </p>
                                                        <!-- Actions -->
                                                        <div class="controls">
                                                            <!-- <div class="like-count">
                                                                <i data-feather="thumbs-up"></i>
                                                                <span>4</span>
                                                            </div>
                                                            <div class="reply">
                                                                <input type="hidden" value="<%= comment._id %> ">
                                                                <a>Reply</a>
                                                            </div>
                                                            <div class="edit">
                                                                <a href="#">Edit</a>
                                                            </div> -->
                                                        </div>
                                                        <!-- Nested Comment -->
                                                        <% comment.altcomment.forEach(altcomment=> { %>
                                                            <div class="media is-comment">
                                                                <!-- User image -->
                                                                <div class="media-left">
                                                                    <div class="image">
                                                                        <img src="/static/uploads/<%= altcomment.user.profilImage %>"
                                                                            data-demo-src="assets/img/avatars/david.jpg" data-user-popover="4" alt="">
                                                                    </div>
                                                                </div>
                                                                <!-- Content -->
                                                                <div class="media-content">
                                                                    <a href="#">
                                                                        <%= altcomment.user.username %>
                                                                    </a>
                                                                    <span class="time">
                                                                        <%= altcomment.getDate() %>
                                                                    </span>
                                                                    <p>
                                                                        <%= altcomment.text %>
                                                                    </p>
                                                                    <!-- Actions -->
                                                                    <div class="controls">
                                                                        <!-- <div class="like-count">
                                                                            <i data-feather="thumbs-up"></i>
                                                                            <span>0</span>
                                                                        </div>
                                                                        <div class="reply">
                                                                            <a onclick="show_alt_comment(this)">Reply</a>
                                                                        </div> -->
                                                                    </div>
                                                                </div>
                                                                <!-- Right side dropdown -->
                                                                <div class="media-right">
                                                                    <div class="dropdown is-spaced is-right is-neutral dropdown-trigger">
                                                                        <div>
                                                                            <div class="button">
                                                                                <i data-feather="more-vertical"></i>
                                                                            </div>
                                                                        </div>
                                                                        <div class="dropdown-menu" role="menu">
                                                                            <div class="dropdown-content">
                                                                                <a class="dropdown-item">
                                                                                    <div class="media">
                                                                                        <i data-feather="x"></i>
                                                                                        <div class="media-content">
                                                                                            <h3>Hide</h3>
                                                                                            <small>Hide this comment.</small>
                                                                                        </div>
                                                                                    </div>
                                                                                </a>
                                                                                <div class="dropdown-divider"></div>
                                                                                <a href="#" class="dropdown-item">
                                                                                    <div class="media">
                                                                                        <i data-feather="flag"></i>
                                                                                        <div class="media-content">
                                                                                            <h3>Report</h3>
                                                                                            <small>Report this comment.</small>
                                                                                        </div>
                                                                                    </div>
                                                                                </a>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <% }) %>
                                                                <div class="card-footer">
                                                                    <div class="media post-comment has-emojis">
                                                                        <!-- Comment Textarea -->
                                                                        <div class="media-content">
                                                                            <div class="field">
                                                                                <p class="control">
                                                                                    <textarea class="textarea comment-textarea" rows="0"
                                                                                        placeholder="Yorum yaz ..."></textarea>
                                                                                </p>
                                                                            </div>
                                                                            <!-- Additional actions -->
                                                                            <div class="actions">
                                                                                <div class="image is-32x32">
                                                                                    <img class="is-rounded"
                                                                                        src="/static/uploads/<%= myuser.profilImage %>"
                                                                                        data-demo-src="assets/img/avatars/jenna.png"
                                                                                        data-user-popover="0" alt="">
                                                                                </div>
                                                                                <div class="toolbar">
                                                                                    <input type="hidden" value="<%= comment._id %>">
                                                                                    <input type="hidden" value="<%= post._id %>">
                                                                                    <a class="button is-solid primary-button"
                                                                                        onclick="add_alt_comment(this)" raised">Alt yorum yap</a>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                    </div>
                                                </div>
                                                <% }) %>
                                
                                
                                                    <!-- /Comment -->
                                
                                
                                        </div>
                                        <div class="card-footer">
                                            <div class="media post-comment has-emojis">
                                                <!-- Comment Textarea -->
                                                <div class="media-content">
                                                    <div class="field">
                                                        <p class="control">
                                                            <textarea class="textarea comment-textarea" rows="1"
                                                                placeholder="Yorum yaz ..."></textarea>
                                                        </p>
                                                    </div>
                                                    <!-- Additional actions -->
                                                    <div class="actions">
                                                        <div class="image is-32x32">
                                                            <img class="is-rounded" src="/static/uploads/<%= myuser.profilImage %>"
                                                                data-demo-src="assets/img/avatars/jenna.png" data-user-popover="0" alt="">
                                                        </div>
                                                        <div class="toolbar">
                                                            <!-- <div class="action is-auto">
                                                                <i data-feather="at-sign"></i>
                                                            </div>
                                                            <div class="action is-emoji">
                                                                <i data-feather="smile"></i>
                                                            </div>
                                                            <div class="action is-upload">
                                                                <i data-feather="camera"></i>
                                                                <input type="file">
                                                            </div> -->
                                                            <input type="hidden" value="<%= post.user._id %>">
                                                            <input type="hidden" value="<%= post._id %>">
                                                            <a class="button is-solid primary-button" onclick="add_comment(this)">Yorum yap</a>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <input type="hidden" name="userid" id="userid" value="<%= myuser._id %>">
                                        <script>
                                            function add_comment(e) {
                                                const userid = document.getElementById("userid").value;
                                                const touserid = e.previousElementSibling.previousElementSibling.value
                                                const postid = e.previousElementSibling.value;
                                                const text = e.parentNode.parentNode.parentNode.children[0].children[0].children[0].value
                                                fetch(`https://basaksocialmediaapi.herokuapp.com/add-comment/${postid}/${userid}`, {
                                                    method: 'POST',
                                                    headers: {
                                                        'Content-Type': 'application/json',
                                                    },
                                                    body: JSON.stringify({ text: text }),
                                                })
                                                    .then(data => {
                                                        document.getElementById(`post-${postid}`).insertAdjacentHTML("beforeend", `
                                                             <div class="media is-comment">
                                                                 <!-- User image -->
                                                                 <div class="media-left">
                                                                     <div class="image">
                                                                         <img src="/static/uploads/<%= myuser.profilImage %>"
                                                                             data-demo-src="assets/img/avatars/dan.jpg" data-user-popover="1" alt="">
                                                                     </div>
                                                                 </div>
                                                                 <!-- Content -->
                                                                 <div class="media-content">
                                                                     <a href="#">
                                                                         <%= myuser.username %>
                                                                     </a>
                                                                     <span class="time">Şimdi</span>
                                                                     <p>
                                                                        ${text}
                                                                     </p>
                                                                 </div>
                                                             </div>
                                                `)
                                                    })
                                                    .catch(err => console.log(err))
                                
                                                    add_nofication("comment",userid, touserid);
                                                
                                            }
                                
                                            function add_alt_comment(e) {
                                                const userid = document.getElementById("userid").value;
                                                const postid = e.previousElementSibling.value;
                                                const commentid = e.previousElementSibling.previousElementSibling.value;
